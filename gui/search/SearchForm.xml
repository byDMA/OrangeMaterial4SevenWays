<gui>

	<!--Поиск-->

	<form id="SearchForm" sourceId="FormWithText">
		<button text="$Search" sourceId="TitleButton"/>
		<button sourceId="ShowMapButton"/>		
		<button sourceId="BackButton"/>
		<button bgPics="orangeUnderline" left="0" right="0" top="35pd" />
		<group sourceId="MenuGroup">
			<menu size="2"  >
				<button sourceId="PicMenuSmallButton" text="$Address" picNormal="find_adress" command="showSearchAddress" bgPics="greyFrame"
                bgPicsPressed="orangeFrame"
                bgColor="Color020"
                bgColorPressed="Color020"
                bgColorDisabled="Color020"
                bgPicsDisabled="greyFrame" />
				
                <button sourceId="PicMenuSmallButton" text="$Coords" picNormal="find_coordinates" command="show(SearchCoordsForm)" 
                bgPics="greyFrame"
                bgPicsPressed="orangeFrame"
                bgColor="Color020"
                bgColorPressed="Color020"
                bgColorDisabled="Color020"/>
                
				<button id="SearchWaypointButton" sourceId="PicMenuSmallButton" text="$MyPlaces" picNormal="find_my_place" picDisabled="find_my_place_d" command="show(SearchWaypointForm)"  bgPics="greyFrame"
                bgPicsPressed="orangeFrame"
                bgColor="Color020"
                bgColorPressed="Color020"
                bgColorDisabled="Color020"
                bgPicsDisabled="greyFrame" />
                
				<button id="SearchHistoryButton" sourceId="PicMenuSmallButton" text="$History" picNormal="find_history" picDisabled="find_history_d" command="show(SearchHistoryForm)" bgPics="greyFrame"
                bgPicsPressed="orangeFrame"
                bgColor="Color020"
                bgColorPressed="Color020"
                bgColorDisabled="Color020"
                bgPicsDisabled="greyFrame" />                
                
                <button sourceId="PicMenuSmallButton" text="$Place" picNormal="find_place" command="showSearchPlace" bgPics="greyFrame"
                bgPicsPressed="orangeFrame"
                bgColor="Color020"
                bgColorPressed="Color020"
                bgColorDisabled="Color020"
                bgPicsDisabled="greyFrame" />
                
				<button sourceId="PicMenuSmallButton" text="$WhatNear" picNormal="find_what_next" command="showNearPlaces" 
                bgPics="greyFrame"
                bgPicsPressed="orangeFrame"
                bgColor="Color020"
                bgColorPressed="Color020"
                bgColorDisabled="Color020"
                bgPicsDisabled="greyFrame" />
                
			</menu>
		</group>
	</form>
</gui>
