<gui>
		<!-- Индикаторы  -->		
		<!-- по-умолчанию  -->
		<indicator fontSize="12" fontColor="Color200"/>
		<!-- индикатор движения по полосам (фон) -->
		<indicator id="BlueFrameIndicator" leftMargin="2pd" rightMargin="2pd" topMargin="2pd" bottomMargin="2pd" bgColor="Color050"
			bgPics="white_pix;white_pix;white_pix;white_pix;;white_pix;white_pix;white_pix;white_pix"/>
		<!-- индикатор расстояния до спидкама -->
		<indicator id="WhiteFrameIndicator" fontColor="black" autoSize="text" textPosition="center;center" 
			bgPics="black_pix;black_pix;black_pix;black_pix;;black_pix;black_pix;black_pix;black_pix"
			bgColor="Color210" leftMargin="1pd" rightMargin="1pd"/>
		
		<!-- индикаторы времени -->
		<indicator id="MapZoomLevelIndicator" title="$MapZoomLevel"/>		
		<indicator id="GPSTimeIndicator" title="$Time"/>
		<indicator id="LocalTimeIndicator" title="$LocalTime"/>
		<indicator id="SystemTimeIndicator" title="$SystemTime"/>
		<indicator id="BaseTimeIndicator" title="$BaseTime"/>
		
		<!-- индикатор спутников -->
		<indicator id="SatellitesIndicator" title="$Satellites"
				fontColor="Color200" fontSize="5" columns="17" interval="2" numberLocation="bottom"
				gpsTextBgColor="Color020" otherTextBgColor="Color020"
				borderColor="Color210" emptyColor="Color210" validColor="Color100" invalidColor="Color080"/>		

		<!-- индикатор ближайшего маневра  -->
        <!-- <indicator id="TurnSchemeIndicator" title="$TurnScheme" fontSize="12" fontColor="Color210"  -->
		<indicator id="TurnSchemeIndicator" title="$TurnScheme" fontSize="14" fontColor="Color210" textPosition="center;center"
				picGoal="goal" picFinish="finish" picRoundabout="roundabout" picMoveForward="move_forward" picTurnBack="turn_back"
				picTurnLeftForward="turn_left_forward" picTurnLeft="turn_left" picTurnLeftBack="turn_left_back"
				picTurnRightForward="turn_right_forward" picTurnRight="turn_right" picTurnRightBack="turn_right_back"
				searchingPics="frame_01; 20; frame_02; 20; frame_03; 20; frame_04; 20; frame_05; 20; frame_06; 20; frame_07; 20; frame_08; 20; frame_09; 20; frame_10; 20; frame_11; 20; frame_12; 20; frame_13; 20; frame_14; 20; frame_15; 20; frame_16; 20; frame_17; 20; frame_18; 20; frame_19; 20; frame_20; 20"
		/>
		<!-- индикатор второго маневра -->
        <!-- <indicator id="SecondTurnIndicator" title="$SecondTurn" fontSize="6" fontColor="Color210" -->
		<indicator id="SecondTurnIndicator" title="$SecondTurn" FontSize="FontSize110" fontColor="Color210" textPosition="center;center"
			picGoal="goal_small" picFinish="finish_small" picRoundabout="roundabout_small" picMoveForward="move_forward_small" picTurnBack="turn_back_small"
			picTurnLeftForward="turn_left_forward_small" picTurnLeft="turn_left_small" picTurnLeftBack="turn_left_back_small"
			picTurnRightForward="turn_right_forward_small" picTurnRight="turn_right_small" picTurnRightBack="turn_right_back_small"
		/>
